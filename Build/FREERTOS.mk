# FreeRTOS Directories
FREERTOSDIR=.\\FreeRTOS\\
FREERTOSSOURCEDIR=$(FREERTOSDIR)Source\\
FREERTOSINCLUDEDIR=$(FREERTOSSOURCEDIR)include\\

ifeq ('$(COMPILER)','ARMGCC')
FREERTOSPORTABLEDIR=portable\\GCC\\$(UPARCHITECTURE)\\
else
FREERTOSPORTABLEDIR=portable\\$(COMPILER)\\$(UPARCHITECTURE)\\
endif

FREERTOSMEMMANGDIR=portable\\MemMang\\
FREERTOSPORTABLE=$(FREERTOSSOURCEDIR)$(FREERTOSPORTABLEDIR)
FREERTOSMEMMANG=$(FREERTOSSOURCEDIR)$(FREERTOSMEMMANGDIR)


